<div class="tp-footer-area {% if center_content %}center-footer-content{% endif %}">
  <div class="tp-footer-top">
    <div class="container">
      <div class="row">
        <div class="col-xl-4 col-lg-3 col-md-5 col-sm-6">
          <div class="tp-footer-widget footer-col-1 mb-50">
            <div class="tp-footer-widget-content">
              <div class="tp-footer-logo">
                <a href="/" aria-label="home">
                  {% set logoUrl = store.logo %}
                  <img loading="lazy" class="footer-logo lazy-img" width="180" height="60"
                    src="{{ image_url(logoUrl,{ h: 175, q: 100 }) }}" alt="{{ store.name }}">
                  {% if footer_logo %}
                  <img loading="lazy" class="footer-another-logo lazy-img" width="180" height="60"
                    src="{{ image_url(footer_logo ,{ h: 175, q: 100 }) }}" alt="{{ store.name }}">
                  {% endif %}
                </a>
              </div>
              <p class="tp-footer-desc">{{ about_settings.about_us_des | safe }}</p>
              <div class="certificates">
                <div class="maroof-icon">
                  {% if store.settings.general.tax_settings.is_certificate_visible %}
                  <div class="vat-icon">
                    <a {% if store.settings.general.tax_settings.tax_registration_certificate %}href="{{ store.settings.general.tax_settings.tax_registration_certificate }}" target="_blank" {% endif %}
                      aria-label="tax_registration_certificate">
                      <img loading="lazy" class="lazy-img" src="{{ 'vat-certificate.png' | asset_url }}"
                        alt="certificate" width="32" height="40">
                    </a>
                  </div>
                  {% endif %}
                  {% if store.settings.general.tax_settings.tax_number %}
                  <div class="text-certificate">
                    <p>{{ _("The VAT number") }}</p>
                    <span>{{ store.settings.general.tax_settings.tax_number }}</span>
                  </div>
                  {% endif %}
                </div>

                {% if store.settings.contact.business_center %}
                <a href="https://eauthenticate.saudibusiness.gov.sa/certificate-details/{{ store.settings.contact.business_center }}"
                  class="maroof-icon" {% if business_center %}target="_blank" {% endif %}>
                  <img loading="lazy" class="lazy-img" src="{{ 'business_center.png' | asset_url }}" alt="" width="32"
                    height="40">
                  <div class="text-certificate">
                    <p>{{ _("Business Certificate") }}</p>
                    <span>{{ store.settings.contact.business_center }}</span>
                  </div>
                </a>
                {% endif %}

                 {% if store.settings.general.commercial_registration_number %}
                <div class="maroof-icon">
                  <div class="text-certificate">
                    <p>{{ _("Commercial registeration number") }}</p>
                    <span>{{ store.settings.general.commercial_registration_number }}</span>
                  </div>
                </div>
                {% endif %}
              </div>
            </div>
          </div>
        </div>
        {% if not (about_settings.links_1_hide) %}
        <div class="col-xl-2 col-lg-3 col-md-3 col-sm-6 col-6">
          <div class="tp-footer-widget">
            <h4 class="tp-footer-widget-title">{{ about_settings.links_1_title }}</h4>
            <div class="tp-footer-widget-content">
              <ul>
                {% for link in about_settings.links_1_links %}
                <li>
                  <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
          </div>
        </div>
        {% endif %}
        {% if not (about_settings.links_2_hide) %}
        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6 col-6">
          <div class="tp-footer-widget">
            <h4 class="tp-footer-widget-title">{{ about_settings.links_2_title }}</h4>
            <div class="tp-footer-widget-content">
              <ul>
                {% for link in about_settings.links_2_links %}
                <li>
                  <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
          </div>
        </div>
        {% endif %}
        {% if not contact_us %}
        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
          <div class="tp-footer-widget footer-col-4 mb-50">
            <h4 class="tp-footer-widget-title">{{ _("Contact us") }}</h4>
            <div class="tp-footer-widget-content">
              <div class="tp-footer-talk">
                <span>
                  {% if about_settings.number_title_title %}
                  {{ about_settings.number_title_title }}
                  {% else %}
                  {{ _("Questions inquiries") }}
                  {% endif %}
                </span>
                <h4>
                  <a href="tel:{{ store.settings.contact.phone }}">{{ store.settings.contact.phone }}</a>
                </h4>
              </div>
              <div class="tp-footer-contact">
                <div class="tp-footer-contact-item">
                  <div class="tp-footer-contact-icon">
                    <svg width="18" height="16" viewbox="0 0 18 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M1 5C1 2.2 2.6 1 5 1H13C15.4 1 17 2.2 17 5V10.6C17 13.4 15.4 14.6 13 14.6H5"
                        stroke="currentColor" stroke-width="1.5" stroke-miterlimit="10" stroke-linecap="round"
                        stroke-linejoin="round"></path>
                      <path d="M13 5.40039L10.496 7.40039C9.672 8.05639 8.32 8.05639 7.496 7.40039L5 5.40039"
                        stroke="currentColor" stroke-width="1.5" stroke-miterlimit="10" stroke-linecap="round"
                        stroke-linejoin="round"></path>
                      <path d="M1 11.4004H5.8" stroke="currentColor" stroke-width="1.5" stroke-miterlimit="10"
                        stroke-linecap="round" stroke-linejoin="round"></path>
                      <path d="M1 8.19922H3.4" stroke="currentColor" stroke-width="1.5" stroke-miterlimit="10"
                        stroke-linecap="round" stroke-linejoin="round"></path>
                    </svg>
                  </div>
                  <div class="tp-footer-contact-content">
                    <a href="mailto:{{ store.settings.contact.email }}">{{ store.settings.contact.email }}</a>
                  </div>
                </div>
                {% if store.settings.general.business_address.show_location %}
                <div class="tp-footer-contact-item">
                  <div class="tp-footer-contact-icon">
                    <svg width="17" height="20" viewbox="0 0 17 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path
                        d="M8.50001 10.9417C9.99877 10.9417 11.2138 9.72668 11.2138 8.22791C11.2138 6.72915 9.99877 5.51416 8.50001 5.51416C7.00124 5.51416 5.78625 6.72915 5.78625 8.22791C5.78625 9.72668 7.00124 10.9417 8.50001 10.9417Z"
                        stroke="currentColor" stroke-width="1.5"></path>
                      <path
                        d="M1.21115 6.64496C2.92464 -0.887449 14.0841 -0.878751 15.7889 6.65366C16.7891 11.0722 14.0406 14.8123 11.6313 17.126C9.88298 18.8134 7.11704 18.8134 5.36006 17.126C2.95943 14.8123 0.210885 11.0635 1.21115 6.64496Z"
                        stroke="currentColor" stroke-width="1.5"></path>
                    </svg>
                  </div>
                  <div class="tp-footer-contact-content">
                    <h4>
                      {% if about_settings.business_location_title %}
													{{ about_settings.business_location_title }}
												{% endif %}
                    </h4>
                    <a {% if store.settings.general.business_address.latitude %} target="_blank"
                      href="https://maps.google.com/?q={{ store.settings.general.business_address.latitude }},{{ store.settings.general.business_address.longitude }}"
                      {% endif %}>
                      {{ store.settings.general.business_address.city.country.name }}, {{
                      store.settings.general.business_address.city.name }}, {{
                      store.settings.general.business_address.district }}, {{
                      store.settings.general.business_address.street }}
                    </a>
                  </div>
                </div>
                {% endif %}
                <div class="tp-footer-social-box">
                  <h4>
                    {% if about_settings.social_media_title %}
                    {{ about_settings.social_media_title }}
                    {% endif %}
                  </h4>
                  <div class="tp-footer-social">
                    {% if store.settings.contact.facebook %}
                    <a href="https://www.facebook.com/{{ store.settings.contact.facebook }}" target="_blank"
                      aria-label="Facebook">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linejoin="round">
                        <path
                          d="M6.18 10.33C5.2 10.33 5 10.52 5 11.44v1.67c0 .92.2 1.11 1.18 1.11h2.36v6.67c0 .92.2 1.11 1.18 1.11h2.36c.98 0 1.18-.19 1.18-1.11v-6.67h2.65c.74 0 .93-.14 1.14-.82l.51-1.67c.35-1.15.13-1.42-.97-1.42h-3.16V7.56c0-.61.53-1.11 1.18-1.11h3.36c.98 0 1.18-.19 1.18-1.11V3.11c0-.92-.2-1.11-1.18-1.11h-3.36c-3.26 0-5.91 2.49-5.91 5.56v2.78H6.18z" />
                      </svg>
                    </a>
                    {% endif %}

                    {% if store.settings.contact.instagram %}
                    <a href="https://www.instagram.com/{{ store.settings.contact.instagram }}"" target=" _blank"
                      aria-label="Instagram">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linejoin="round">
                        <path
                          d="M2.5 12C2.5 7.52 2.5 5.28 3.89 3.89 5.28 2.5 7.52 2.5 12 2.5s9.5 0 10.11 1.39C21.5 5.28 21.5 7.52 21.5 12s0 9.5-1.39 10.11C18.72 21.5 16.48 21.5 12 21.5s-9.5 0-10.11-1.39C2.5 18.72 2.5 16.48 2.5 12Z" />
                        <path d="M16.5 12a4.5 4.5 0 1 1-9 0 4.5 4.5 0 0 1 9 0Z" />
                        <circle cx="17.5" cy="6.5" r="1" />
                      </svg>
                    </a>
                    {% endif %}

                    {% if store.settings.contact.twitter %}
                    <a href="https://twitter.com/{{ store.settings.contact.twitter }}" target="_blank"
                      aria-label="Twitter">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                        <path
                          d="M3 21L10.5 13.5M21 3L13.5 10.5M13.5 10.5L8 3H3L10.5 13.5M13.5 10.5L21 21H16L10.5 13.5" />
                      </svg>
                    </a>
                    {% endif %}

                    {% if store.settings.contact.snapchat %}
                    <a href="https://www.snapchat.com/add/{{ store.settings.contact.snapchat }}" target="_blank"
                      aria-label="Snapchat">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                        <path
                          d="M6.6 7.4C6.6 4.4 9 2 12 2s5.4 2.4 5.4 5.4c0 4.8.2 7.2 4.6 9-2.2.6-2.8 1.2-3.3 3.4-3.9 0-4.5 2.2-6.7 2.2s-3.3-2.2-7.2-2.2c-.5-2.2-1-2.8-3.3-3.4 4.4-1.8 4.6-4.2 4.6-9Z" />
                        <path d="M2 16c3.8-2.1 3.8-4.1 1-7" />
                        <path d="M22 16c-3.8-2.1-3.8-4.1-1-7" />
                      </svg>
                    </a>
                    {% endif %}

                    {% if store.settings.contact.tiktok %}
                    <a href="https://www.tiktok.com/@{{ store.settings.contact.tiktok }}" target="_blank"
                      aria-label="TikTok">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                        <path
                          d="M2.5 12c0-4.48 0-6.72 1.39-8.11C5.28 2.5 7.52 2.5 12 2.5s6.72 0 8.11 1.39C21.5 5.28 21.5 7.52 21.5 12s0 6.72-1.39 8.11C18.72 21.5 16.48 21.5 12 21.5s-6.72 0-8.11-1.39C2.5 18.72 2.5 16.48 2.5 12Z" />
                        <path
                          d="M10.5 11c-.82-.12-2.69.07-3.6 1.77-.91 1.7-.03 3.45.54 4.12.57.63 2.38 1.85 4.3.69.47-.29 1.06-.52 1.73-2.76V6c-.13.98.95 3.26 4 3.5" />
                      </svg>
                    </a>
                    {% endif %}
                    {% if store.settings.contact.website %}
                    <a href="{{ store.settings.contact.website }}" target="_blank" aria-label="Website">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" fill="none"
                        stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                        <circle cx="12" cy="12" r="10" />
                        <path d="M8 12c0 6 4 10 4 10s4-4 4-10S12 2 12 2 8 6 8 12Z" />
                        <path d="M21 15H3M21 9H3" />
                      </svg>
                    </a>
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        {% endif %}
      </div>
      {% if store.settings.general.one_signal.ios_app_id or store.settings.general.one_signal.android_package_name%}
      <div class="google-apple-stores">
        {% if store.settings.general.one_signal.ios_app_id %}
        <a href="https://apps.apple.com/app/id{{ store.settings.general.one_signal.ios_app_id }}" target="_blank"
          aria-label="ios_app">
          <img loading="lazy" class="lazy-img" src="{{ 'app-store.svg' | asset_url }}" alt="store" width="115"
            height="35">
        </a>
        {% endif %}
        {% if store.settings.general.one_signal.android_package_name %}
        <a href="https://play.google.com/store/apps/details?id={{ store.settings.general.one_signal.android_package_name }}"
          target="_blank">
          <img src="{{ 'google-play.svg' | asset_url }}" loading="lazy" alt="store" width="115" height="35">
        </a>
        {% endif %}
      </div>
      {% endif %}

    </div>
  </div>
  <div class="tp-footer-bottom">
    <div class="container">
      <div class="tp-footer-bottom-wrapper">
        <div class="tp-footer-copyright">
          {{ store.settings.branding.copyrights | safe}}
        </div>
        {% if store.settings.checkout.shipping_methods or store.settings.checkout.payment_methods %}
        <div class="tp-footer-payment">
          {% if not shipping_icons %}
           {% for shipping_method in store.settings.checkout.shipping_methods %}
          <a href="/shipping-and-payment" aria-label="shipping_method">
            <img loading="lazy" class="lazy-img" src="{{ image_url(shipping_method.icon,{ h: 80, q: 100 }) }}"
              alt="shipping" width="26" height="26">
          </a>
          {% endfor %}
          {% endif %}
          {% for payment_method in store.settings.checkout.payment_methods %}
          <a href="/shipping-and-payment" aria-label="payment_methods">
            <img loading="lazy" class="lazy-img" src="{{ image_url(payment_method.icon,{ h: 80, q: 100 }) }}"
              alt="payment" width="26" height="26">
          </a>
          {% endfor %}
        </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>