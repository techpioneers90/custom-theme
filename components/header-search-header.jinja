  <script>
    const searchInput = document.querySelector('.search-input-input');
    const suggestions = document.querySelector('.suggestions');
    const overlaySearch = document.querySelector('.overlay-search-style2');
    const autocompleteItems = document.querySelector('.autocomplete-items');
    const placeholderTag = document.querySelector('.placeholder-tag');

    searchInput.addEventListener('focus', () => {
      if (!autocompleteItems.classList.contains('has-data')) {
        suggestions.classList.add('active');
      }
      overlaySearch.classList.add('active');
    });

    searchInput.addEventListener('input', () => {
      if (searchInput.value.trim() === '') {
        suggestions.classList.add('active');
        placeholderTag.style.display = 'block';
      } else {
        suggestions.classList.remove('active');
        placeholderTag.style.display = 'none';
      }
    });

    overlaySearch.addEventListener('click', () => {
      suggestions.classList.remove('active');
      overlaySearch.classList.remove('active');
    });
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/2.0.8/typed.min.js"></script>
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      var elementSpan = document.querySelector(".placeholder-tag em");
      if (!elementSpan) return;
      
      var rawString = elementSpan.getAttribute("data-elements-span");
      var stringsWord = rawString.split("|");
      
      var optionsTyped = {
        strings: stringsWord,
        typeSpeed: 50,
        backSpeed: 1,
        backDelay: 1500,
        loop: true
      };

      new Typed(".placeholder-tag em", optionsTyped);
    });
  </script>
